package Practice;

import java.util.HashMap;
import java.util.Map;

public class Test3 extends PutStatsMapDecorator<Object, Object> {
	public Test3(Map<Object, Object> map) {
		super(map);
	}

	public static void main(String[] args) {
		PutStatsMapDecorator<String, Integer> psmd = new PutStatsMapDecorator<>(new HashMap<>());
		psmd.put("A", 1);
		psmd.put("B", 2);
		psmd.putIfAbsent("A", 4);
		System.out.println(psmd.getCountPut());
		System.out.println(psmd.getCountPutAll());
		System.out.println(psmd.getCountPutIfAbsent());
		System.out.println(psmd.get("A"));
	}
}