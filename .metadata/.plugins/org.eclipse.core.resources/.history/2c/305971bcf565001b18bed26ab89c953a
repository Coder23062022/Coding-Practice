package Practice;

import java.util.HashMap;
import java.util.Map;

public class Test3 extends PutStatsMapDecorator<Object, Object> {
	public Test3(Map map) {
		super(map);
		// TODO Auto-generated constructor stub
	}

	public static void main(String[] args) {
		Map<String, Integer> map = new HashMap<>();
//		map.put("A", 1);
//		map.put("B", 2);

		PutStatsMapDecorator<String, Integer> psmd = new PutStatsMapDecorator<>(map);
		psmd.put("A", 1);
		psmd.put("B", 2);

		System.out.println(psmd.getCountPut());
	}
}