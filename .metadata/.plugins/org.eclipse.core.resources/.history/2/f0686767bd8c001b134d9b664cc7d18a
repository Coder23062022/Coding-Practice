package Practice;

import java.util.ArrayList;
import java.util.Arrays;

public class Test1 {
	public static void main(String[] args) {
		/*
		 * int arr[] = { 5, 4, 4, 2, 2, 8 }; Arrays.sort(arr);
		 * System.out.println(arr.length); for (int i = 1; i < arr.length; i++) { if
		 * (arr[i] != arr[i - 1]) { System.out.println(arr.length - i); } }
		 */
		System.out.println(permutation("ABC"));
	}

	public static ArrayList<String> permutation(String S) {
		ArrayList<String> al = new ArrayList<String>();
		for (int i = 0; i < S.length(); i++) {
			al.add(permutation(S.charAt(0), S.substring(0, i) + S.substring(i + 1, S.length())));
		}
		return al;
	}
}