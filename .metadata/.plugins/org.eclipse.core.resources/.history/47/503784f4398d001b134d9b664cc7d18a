package Practice;

import java.util.ArrayList;

public class Test1 {
	public static void main(String[] args) {

		/*
		 * int arr[] = { 5, 4, 4, 2, 2, 8 }; Arrays.sort(arr);
		 * System.out.println(arr.length); for (int i = 1; i < arr.length; i++) { if
		 * (arr[i] != arr[i - 1]) { System.out.println(arr.length - i); } }
		 */
		System.out.println(permutation("GOD"));
	}

	public static ArrayList<String> permutation(String S) {
		ArrayList<String> al = new ArrayList<String>();
		al.addAll(permutation1("", S));
		return al;
	}

	private static ArrayList<String> permutation1(String perm, String word) {
		ArrayList<String> al = new ArrayList<String>();
		if (word.isEmpty()) {
			System.err.println(perm + word);
		} else {
			for (int i = 0; i < word.length(); i++) {
				al.addAll(permutation1(perm + word.charAt(i),
						word.substring(0, i) + word.substring(i + 1, word.length())));
			}
		}
		return al;
	}
}